{%
  set classes = [
    dom_id ? 'js-view-dom-id-' ~ dom_id,
	'wap-views'
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}

  {% if header %}
    
      {{ header }}
    
  {% endif %}

  {{ exposed }}
  {{ attachment_before }}

  {{ rows }}
  {{ empty }}
  {{ pager }}

  {{ attachment_after }}
  {{ more }}

  {% if footer %}
      {{ footer }}
  {% endif %}

  {{ feed_icons }}
</div>